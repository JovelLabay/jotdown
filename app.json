{
  "expo": {
    "name": "Jotdown",
    "slug": "jotdown",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/jotdownLogo.png",
    "splash": {
      "image": "./assets/jotdownSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-jotdown-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.jovellabay.jotdown"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
